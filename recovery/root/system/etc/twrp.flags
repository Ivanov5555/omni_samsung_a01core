/loader_ext1            emmc      /dev/block/platform/bootdevice/by-name/loader_ext1   flags=backup=1;flashimg;display="loader_ext1"
/loader_ext2            emmc      /dev/block/platform/bootdevice/by-name/loader_ext2   flags=backup=1;flashimg;display="loader_ext2"
/metadata               emmc      /dev/block/platform/bootdevice/by-name/metadata      flags=backup=1;flashimg;display="metadata"
/param                  emmc      /dev/block/platform/bootdevice/by-name/param         flags=backup=1;flashimg;display="param"
/persist                emmc      /dev/block/platform/bootdevice/by-name/persist       flags=backup=1;flashimg;display="persist"

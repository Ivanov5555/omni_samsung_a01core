/preloader_b            emmc      /dev/block/platform/bootdevice/by-name/preloader_b   flags=backup=1;flashimg;display="preloader_b";
/reserved               emmc      /dev/block/platform/bootdevice/by-name/reserved      flags=backup=1;flashimg;display="reserved"
/sec1                   emmc      /dev/block/platform/bootdevice/by-name/sec1          flags=backup=1;flashimg;display="sec1"
/steady                 emmc      /dev/block/platform/bootdevice/by-name/steady        flags=backup=1;flashimg;display="steady"
/vbmeta                 emmc      /dev/block/platform/bootdevice/by-name/vbmeta        flags=backup=1;flashimg;display="vbmeta";
/vbmeta_system          emmc      /dev/block/platform/bootdevice/by-name/vbmeta_system flags=backup=1;flashimg;display="vbmeta_system"
/vbmeta_vendor          emmc      /dev/block/platform/bootdevice/by-name/vbmeta_vendor flags=backup=1;flashimg;display="vbmeta_vendor"

# Full dump
/mmcblk0                emmc      /dev/block/platform/bootdevice/mmcblk0               flags=display="Full dump";backup=1;flashimg

# Removable storage

/external_sd            auto      /dev/block/mmcblk1p1        /dev/block/mmcblk1       flags=storage;removable;display="sdcard";wipeingui;
/usb-otg                auto      /dev/block/sda1             /dev/block/sda           flags=display="USB-OTG";storage;removable;wipeingui;

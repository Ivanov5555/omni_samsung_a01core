# mount point          fstype                                   device                                      flags

# Dynamic Partitions

/system_root            ext4      /dev/block/mapper/system  flags=backup=0;display="System"
/system_image           emmc      /dev/block/mapper/system  flags=backup=1;flashimg;display="System Image";
/vendor                 ext4      /dev/block/mapper/vendor  flags=backup=0;display="Vendor"
/vendor_image           emmc      /dev/block/mapper/vendor  flags=backup=1;flashimg;display="Vendor Image";
/product                ext4      /dev/block/mapper/product  flags=backup=0;display="Product";
/product_image          emmc      /dev/block/mapper/product  flags=backup=1;flashimg;display="Product Image";
/odm                    ext4      /dev/block/mapper/odm    flags=backup=0;display="ODM";
/odm_image              emmc      /dev/block/mapper/odm flags=backup=1;flashimg;display="ODM Image";
 
/prism                  emmc      /dev/block/platform/bootdevice/by-name/prism         flags=backup=1;flashimg;display="prism";
/optics                 emmc      /dev/block/platform/bootdevice/by-name/optics        flags=backup=1;flashimg;display="optics";
/md_udc                 emmc      /dev/block/platform/bootdevice/by-name/md_udc        flags=backup=1;flashimg;display="md_udc";
/cache                  ext4      /dev/block/platform/bootdevice/by-name/cache         flags=backup=0;display="cache";
/cache                  emmc      /dev/block/platform/bootdevice/by-name/cache         flags=backup=1;flashimg;display="cache";
/data                   f2fs      /dev/block/platform/bootdevice/by-name/userdata      flags=backup=1;usermrf;removable;display="Userdata";wipeingui;length=-16384
/protect_1              emmc      /dev/block/platform/bootdevice/by-name/protect1      flags=backup=1;flashimg;display="protect_1";
/protect_2              emmc      /dev/block/platform/bootdevice/by-name/protect2      flags=backup=1;flashimg;display="protect_2";
/nvdata                 emmc      /dev/block/platform/bootdevice/by-name/nvdata        flags=backup=1;flashimg;display="nvdata";
/nvcfg                  emmc      /dev/block/platform/bootdevice/by-name/nvcfg         flags=backup=1;flashimg;display="nvcfg";
/efs                    emmc      /dev/block/platform/bootdevice/by-name/efs           flags=backup=1;flashimg;display="efs";
/omr                    emmc      /dev/block/platform/bootdevice/by-name/omr           flags=backup=1;flashimg;display="omr";
/sec_efs                emmc      /dev/block/platform/bootdevice/by-name/sec_efs       flags=backup=1;flashimg;display="sec_efs";
/storage                f2fs      /data/media/0                                        flags=display="Internal Storage";backup=1;flashimg=1;fsflags="bind";usermrf;removable;


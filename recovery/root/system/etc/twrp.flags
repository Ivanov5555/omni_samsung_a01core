/gz2                    emmc      /dev/block/platform/bootdevice/by-name/gz2           flags=backup=1;flashimg;display="gz2"
/spmfw                  emmc      /dev/block/platform/bootdevice/by-name/spmfw         flags=backup=1;flashimg;display="spmfw"
/mcupmfw                emmc      /dev/block/platform/bootdevice/by-name/mcupmfw       flags=backup=1;flashimg;display="mcupmfw"
/boot_para              emmc      /dev/block/platform/bootdevice/by-name/boot_para     flags=backup=1;flashimg;display="boot_para"
/dtbo                   emmc      /dev/block/platform/bootdevice/by-name/dtbo          flags=backup=1;flashimg;display="dtbo"

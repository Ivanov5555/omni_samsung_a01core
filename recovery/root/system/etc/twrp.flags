/proinfo                emmc      /dev/block/platform/bootdevice/by-name/proinfo       flags=backup=1;flashimg;display="proinfo";
/lk                     emmc      /dev/block/platform/bootdevice/by-name/lk            flags=backup=1;flashimg;display="lk";
/para                   emmc      /dev/block/platform/bootdevice/by-name/para          flags=backup=1;flashimg;display="para"
/misc                   emmc      /dev/block/platform/bootdevice/by-name/misc          flags=backup=1;flashimg;display="misc"
/boot                   emmc      /dev/block/platform/bootdevice/by-name/boot          flags=backup=1;flashimg;display="boot";
/recovery               emmc      /dev/block/platform/bootdevice/by-name/recovery      flags=backup=1;flashimg;display="recovery";
/expdb                  emmc      /dev/block/platform/bootdevice/by-name/expdp         flags=backup=1;flashimg;display="expdb";
